{
  "module": {
    "name": "go_nomads_app",
    "type": "entry",
    "description": "$string:module_desc",
    "mainElement": "Go_nomads_appAbility",
    "deviceTypes": [
      "phone"
    ],
    "deliveryWithInstall": true,
    "installationFree": false,
    "pages": "$profile:main_pages",
    "abilities": [
      {
        "name": "Go_nomads_appAbility",
        "srcEntry": "./ets/go_nomads_appability/Go_nomads_appAbility.ets",
        "description": "$string:Go_nomads_appAbility_desc",
        "icon": "$media:layered_image",
        "label": "$string:Go_nomads_appAbility_label",
        "startWindowIcon": "$media:startIcon",
        "startWindowBackground": "$color:start_window_background",
        "exported": true,
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "ohos.want.action.home"
            ]
          }
        ]
      }
    ],
    "extensionAbilities": [
      {
        "name": "Go_nomads_appBackupAbility",
        "srcEntry": "./ets/go_nomads_appbackupability/Go_nomads_appBackupAbility.ets",
        "type": "backup",
        "exported": false,
        "metadata": [
          {
            "name": "ohos.extension.backup",
            "resource": "$profile:backup_config"
          }
        ],
      }
    ],
    "requestPermissions": [
      {
        "name": "ohos.permission.INTERNET"
      },
      {
        "name": "ohos.permission.GET_NETWORK_INFO"
      }
    ]
  }
}